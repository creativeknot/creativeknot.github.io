{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "src/",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@config": ["config.ts"]
    },
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "plugins": [{
      "name": "@astrojs/ts-plugin"
    }]
  }
}
