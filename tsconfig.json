{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "src/",
    "paths": {
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@pages/*": ["pages/*"],
      "@css/*": ["css/*"],
      "@config": ["config.ts"]
    },
    "jsxImportSource": "solid-js",
    "jsx": "preserve"
  }
}
